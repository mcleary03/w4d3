<h1>Log In</h1>

<form action=" <%= session_url(@session) %>" method="post" id="new-session">
  <label for="user_name">User Name
    <input type="hidden" name="authenticity_token" value="form_authenticity_token">
    <input type="text" name="session[user_name]" >
  </label>
  <label for="password">Password
    <input type="password" name="session[password]">
  </label>
  <button type="submit" name="submit" form="new-session">Log In</button>
</form>
